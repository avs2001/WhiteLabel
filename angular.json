    "AdminPortal": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/admin-portal",
      "sourceRoot": "projects/admin-portal/src",
      "prefix": "kbm",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/admin-portal",
            "index": "projects/admin-portal/src/index.html",
            "main": "projects/admin-portal/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/admin-portal/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/admin-portal/src/favicon.ico",
              "projects/admin-portal/src/assets",
              {
                "glob": "**/*",
                "input": "dist/common/ui/assets",
                "output": "assets"
              }
            ],
            "styles": [
              "dist/common/ui/styles/styles.scss",
              "dist/common/ui/styles/main-list.scss",
              "projects/admin-portal/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "AdminPortal:build:production"
            },
            "development": {
              "browserTarget": "AdminPortal:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "AdminPortal:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/admin-portal/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/admin-portal/src/favicon.ico",
              "projects/admin-portal/src/assets"
            ],
            "styles": [
              "projects/admin-portal/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
